  "version": "0.2.0",
  "configurations": [
    {
      "name": "(python) em_py",
      "type": "python",
      "request": "launch",
      "module": "unittest",
      "args": [ 
        "-v", "test.testPycalc.test_em_py"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/tests/python/debug",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/lib/Debug/python"
      }
    },
    {
      "name": "(python) smoother_py",
      "type": "python",
      "request": "launch",
      "module": "unittest",
      "args": [ 
        "-v", "test.testPycalc.test_kalmansmoother_py"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/tests/python/debug",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/lib/Debug/python"
      }
    },
    {
      "name": "(gdb) test test_cfp",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Debug/bin/test_cfp",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/opt/gcc84/bin/gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    }
  ]
}